{
    "name": "javaabu/query-builder",
    "description": "Modifications on top of spatie/query-builder",
    "homepage": "https://github.com/Javaabu/query-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Athfan Khaleel",
            "email": "athfan@javaabu.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0",
        "spatie/laravel-query-builder": "^5.4"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\QueryBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\QueryBuilder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\QueryBuilder\\QueryBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
