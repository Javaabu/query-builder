{
    "name": "javaabu/query-builder",
    "description": "Modifications on top of spatie/query-builder",
    "homepage": "https://github.com/Javaabu/query-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Athfan Khaleel",
            "email": "athfan@javaabu.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "spatie/laravel-query-builder": "^6.3"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^10.5 || ^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\QueryBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\QueryBuilder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\QueryBuilder\\QueryBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
